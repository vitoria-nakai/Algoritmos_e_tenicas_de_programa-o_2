/*
Escreva um programa que leia o conteúdo do arquivo "nomes.txt" 
criado no exercício anterior e exiba os nomes na tela.
*/

#include<stdio.h>
#include<stdlib.h>

int main(){
    FILE * file;
    file = fopen("nomes.txt", "r");
    char str[50];
    
    printf("Os nomes do arquivo são: \n");
    for(int i = 0; i < 5; i++){
        fgets(str, sizeof(str), file);
        printf("%s", str);
    }
    getchar();
    return 0;
}
